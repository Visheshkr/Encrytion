#include<stdio.h>
#include<math.h>
#include<string.h>
int main()
{
    char s[100],a[100][100];
    int i,j,k,p=0,l;
    gets(s);
    for(i=0;s[i]!='\0';i++)
    {
        if(s[i]==' ')
        {
        for(j=i;s[i]!='\0';j++)
        {
            s[j]=s[j+1];
        }
        }
    }
    l=strlen(s);
    p=sqrt(l);
    k=0;
    if(p*p==l)
    {
        for(i=0;i<p;i++)
        {
            for(j=0;j<p;j++)
            {
                a[i][j]=s[k];
                k++;
            }
        }
    }
    else if(p*(p+1)>=l)
    {
        for(i=0;i<p;i++)
        {
            for(j=0;j<p+1;j++)
            {
                a[i][j]=s[k];
                k++;
            }
        }
    }
    else
    {
        for(i=0;i<p+1;i++)
        {
            for(j=0;j<p+1;j++)
            {
                a[i][j]=s[k];
                k++;
            }
        }
    }
    for(i=0;i<=p;i++)
    {
        for(j=0;j<=p;j++)
        {
            if(a[j][i]!='\0')
            printf("%c",a[j][i]);
        }
        printf(" ");
    }

return 0;
}
